<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Yeetmedia3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Yeetmedia3"
    xmlns:views="clr-namespace:Yeetmedia3.Views"
    Title="YeetMedia3">

    <FlyoutItem Title=".NET Rocks" Icon="dotnet_bot.png">
        <ShellContent
            ContentTemplate="{DataTemplate views:DotnetRocksView}"
            Route="DotnetRocksView" />
    </FlyoutItem>

    <FlyoutItem Title="Google Drive" Icon="dotnet_bot.png">
        <ShellContent
            ContentTemplate="{DataTemplate views:GoogleDriveView}"
            Route="GoogleDriveView" />
    </FlyoutItem>

</Shell>
